<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/geogebra/db" COMMENT="" VERSION="20070401" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
<!--
id BIGINT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
course BIGINT(10) UNSIGNED NOT NULL,
name VARCHAR(255) CHARACTER SET 'utf8' COLLATE 'utf8_general_ci' NOT NULL,
url VARCHAR(1020) CHARACTER SET 'utf8' COLLATE 'utf8_general_ci' NOT NULL,
width BIGINT(1) UNSIGNED NOT NULL,
height BIGINT(1) UNSIGNED NOT NULL,
showsubmit TINYINT(1) UNSIGNED NOT NULL,



timecreated BIGINT(10) UNSIGNED NOT NULL,
timemodified BIGINT(10) UNSIGNED NOT NULL,
PRIMARY KEY(`id`),
INDEX(`course`)
-->
    <TABLES>
        <TABLE NAME="geogebra" NEXT="geogebra_attempts" COMMENT="">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="course" COMMENT=""/>
                <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="name" COMMENT=""/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="course" NEXT="intro" COMMENT=""/>
                <FIELD NAME="intro" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="name" NEXT="url"/>
                <FIELD NAME="url" TYPE="char" LENGTH="1024" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="intro" NEXT="width" COMMENT=""/>
                <FIELD NAME="width" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="url" NEXT="height" COMMENT=""/>
                <FIELD NAME="height" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="width" NEXT="showsubmit" COMMENT=""/>                
                <FIELD NAME="showsubmit" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="height" NEXT="maxattempts" COMMENT="" />                            
                <FIELD NAME="maxattempts" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" PREVIOUS="showsubmit" NEXT="grademethod" COMMENT=""/>
                <FIELD NAME="grademethod" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" PREVIOUS="maxattempts" NEXT="maxgrade" COMMENT=""/>
                <FIELD NAME="maxgrade" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" PREVIOUS="grademethod" NEXT="timeavailable" COMMENT=""/>
                <FIELD NAME="timeavailable" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="maxgrade" NEXT="timedue" COMMENT=""/>
                <FIELD NAME="timedue" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timeavailable" NEXT="timecreated" COMMENT=""/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timedue" NEXT="timemodified" COMMENT=""/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timecreated" COMMENT=""/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
            <INDEXES>
                <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
            </INDEXES>
        </TABLE>
		
        <TABLE NAME="geogebra_attempts" PREVIOUS="geogebra" COMMENT="">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="geogebra" COMMENT=""/>
                <FIELD NAME="geogebra" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="userid" COMMENT=""/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="geogebra" NEXT="vars" COMMENT=""/>
                <FIELD NAME="vars" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="finished" COMMENT="" />
                <FIELD NAME="finished" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false" PREVIOUS="vars" NEXT="date" COMMENT=""/>
                <FIELD NAME="date" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="finished" COMMENT=""/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
            <INDEXES>
                <INDEX NAME="geogebra" UNIQUE="false" FIELDS="geogebra"/>
            </INDEXES>
        </TABLE>
    </TABLES>
	
    <STATEMENTS>
        <STATEMENT NAME="insert log_display" TYPE="insert" TABLE="log_display">
            <SENTENCES>
                <SENTENCE TEXT="(module, action, mtable, field) VALUES ('geogebra', 'add', 'geogebra', 'name')" />
                <SENTENCE TEXT="(module, action, mtable, field) VALUES ('geogebra', 'update', 'geogebra', 'name')" />
                <SENTENCE TEXT="(module, action, mtable, field) VALUES ('geogebra', 'view', 'geogebra', 'name')" />
            </SENTENCES>
        </STATEMENT>
    </STATEMENTS>
</XMLDB>
